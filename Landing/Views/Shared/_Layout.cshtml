                                <li>
                                    <a href="/landing?q=گارانتی کسری سرویس">گارانتی کسری سرویس</a>
                                </li>
                                <li>
                                    <a href="/landing?q=بازرگانی خضر">بازرگانی خضر</a>
                                </li>
                                <li>
                                    <a href="/landing?q=کسری تامین اندیشان">کسری تامین اندیشان</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-xl-4 col-md-6">
                        <div class="optech-footer-menu dark-color">
                            <div class="optech-footer-title dark-color">
                                <h5>خدمات</h5>
                            </div>
                            <ul>
                                <li>
                                    <a href="/landing?q=واردات کالا">واردات کالا</a>
                                </li>
                                <li>
                                    <a href="/landing?q=قیمت گذاری و توزیع کالا">قیمت گذاری و توزیع کالا</a>
                                </li>
                                <li>
                                    <a href="/landing?q=ضمانت و گارانتی کالا">ضمانت و گارانتی کالا</a>
                                </li>
                                <li>
                                    <a href="/landing?q=فروش و توزیع کالا">فروش و توزیع کالا</a>
                                </li>

                            </ul>
                        </div>
                    </div>
                }
                else
                {
                    <div class="col-xl-3 offset-xl-1 col-md-6">
                        <div class="optech-footer-menu dark-color">
                            <div class="optech-footer-title dark-color">
                                <h5>Kasra Group</h5>
                            </div>
                            <ul>
                                <li>
                                    <a href="/landing?q=Company Introduction">Company Introduction</a>
                                </li>
                                <li>
                                    <a href="/landing?q=Kasra Computer">Kasra Computer</a>
                                </li>
                                <li>
                                    <a href="/landing?q=Parsme Shop">Parsme Shop</a>
                                </li>
                                <li>
                                    <a href="/landing?q=Kasra Tamin Andishan ">Kasra Tamin Andishan </a>
                                </li>
                                <li>
                                    <a href="/landing?q=Khezr Trading ">Khezr Trading </a>
