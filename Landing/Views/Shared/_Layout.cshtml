                    </div>
                </div>
                @if (CultureInfo.CurrentCulture.Name == "fa")
                {
                    <div class="col-xl-3 offset-xl-1 col-md-6">
                        <div class="optech-footer-menu dark-color">
                            <div class="optech-footer-title dark-color">
                                <h5>گروه کسری</h5>
                            </div>
                            <ul>
                                <li>
                                    <a href="/landing?q=معرفی شرکت">معرفی شرکت</a>
                                </li>
                                <li>
                                    <a href="/landing?q=کسری کامپیوتر">کسری کامپیوتر</a>
                                </li>
                                <li>
                                    <a href="/landing?q=فروشگاه اینترنتی پارس می">فروشگاه اینترنتی پارس می</a>
